{
    "name": "mindtwo/two-tility",
    "description": "Utility package by mindtwo",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "mindtwo\\TwoTility\\": "src/",
            "mindtwo\\TwoTility\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3|^8.4",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\TwoTility\\TwoTilityProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
