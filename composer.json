{
    "name": "mindtwo/two-tility",
    "description": "Utility package by mindtwo",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "mindtwo\\TwoTility\\": "src/",
            "mindtwo\\TwoTility\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.28|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.7|^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/pint": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\TwoTility\\Providers\\TwoTilityProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
